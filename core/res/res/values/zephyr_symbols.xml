<resources>
  <!-- Power menu reboot -->
  <java-symbol name="reboot_confirm" type="string" />
  <java-symbol name="reboot_title" type="string" />
  <java-symbol name="ic_lock_power_reboot" type="drawable" />
  <java-symbol name="global_action_reboot" type="string" />
  <java-symbol type="array" name="shutdown_reboot_options" />
  <java-symbol type="array" name="shutdown_reboot_actions" />
  <java-symbol type="drawable" name="ic_lock_screenshot" />
  <java-symbol name="global_action_screenshot" type="string" />
  <java-symbol type="string" name="reboot_progress" />
  <java-symbol type="bool" name="config_advanced_reboot" />
  <java-symbol type="drawable" name="ic_lock_settings" />
  <java-symbol type="drawable" name="ic_lock_user" />

  <!-- We don't want to publish private symbols in android.R as part of the
         SDK.  Instead, put them here. -->
  <private-symbols package="com.android.internal" />

  <!-- Private symbols that we need to reference from framework code.  See
         frameworks/base/core/res/MakeJavaSymbols.sed for how to easily generate this.  
-->
  <!-- ZephyrOS -->
  <java-symbol type="drawable" name="logo_zephyr" />

  <!-- Boot dexopt layout -->
  <java-symbol type="drawable" name="dexopt_brand_logo" />
  <java-symbol type="color" name="dexopt_background" />
  <java-symbol type="color" name="dexopt_text" />
  <java-symbol type="dimen" name="dexopt_icon_margin" />
  <java-symbol type="dimen" name="dexopt_icon_size" />
  <java-symbol type="dimen" name="dexopt_logo_size" />
  <java-symbol type="dimen" name="dexopt_message" />
  <java-symbol type="layout" name="boot_dexopt_layout" />
  <java-symbol type="id" name="dexopt_icon" />
  <java-symbol type="id" name="dexopt_message" />
  <java-symbol type="id" name="dexopt_message_detail" />
  <java-symbol type="id" name="dexopt_progress" />

  <!-- Notification and battery light -->
  <java-symbol type="bool" name="config_intrusiveNotificationLed" />
  <java-symbol type="bool" name="config_multiColorNotificationLed" />
  <java-symbol type="bool" name="config_intrusiveBatteryLed" />
  <java-symbol type="bool" name="config_multiColorBatteryLed" />
  <java-symbol type="array" name="notification_light_package_mapping" />
